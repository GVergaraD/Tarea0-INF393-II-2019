    "import math\n",
    "\n",
    "def sigmoid(x):\n",
    "    if x<-709:\n",
    "        return 0\n",
    "    return 1 / (1 + math.exp(-x))\n",
    "\n",
    "def f2(X,W):\n",
    "    y = np.dot(X,W) #producto punto entre X y W\n",
    "    if (sigmoid(y) >= 0):\n",
    "        return 1\n",
    "    else:\n",
